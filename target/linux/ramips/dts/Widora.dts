/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
    compatible = "mediatek,widora", "mediatek,mt7628an-soc";
    model = "Widora-NEO";

    chosen {
        bootargs = "console=ttyS2,115200";
    };

    memory@0 {
        device_type = "memory";
        reg = <0x0 0x8000000>;
    };

    pinctrl {
        state_default: pinctrl0 {
            gpio {
                ralink,group = "gpio";
                ralink,function = "gpio";
            };
            perst {
                ralink,group = "perst";
                ralink,function = "gpio";
            };
            refclk {
                ralink,group = "refclk";
                ralink,function = "gpio";
            };
            i2s {
                ralink,group = "i2s";
                ralink,function = "gpio";
            };
            spis {
                ralink,group = "spis";
                ralink,function = "gpio";
            };
            wled_an {
                ralink,group = "wled_an";
                ralink,function = "wled_an";
            };
            ephyk_p1 {
                ralink,group = "ephyk_p1";
                ralink,function = "gpio";
            };
            ephyk_p2 {
                ralink,group = "ephyk_p2";
                ralink,function = "gpio";
            };
            ephyk_p3 {
                ralink,group = "ephyk_p3";
                ralink,function = "gpio";
            };
            ephyk_p4 {
                ralink,group = "ephyk_p4";
                ralink,function = "gpio";
            };
            ephy_p1 {
                ralink,group = "ephy_p1";
                ralink,function = "gpio";
            };
            ephy_p2 {
                ralink,group = "ephy_p2";
                ralink,function = "gpio";
            };
            ephy_p3 {
                ralink,group = "ephy_p3";
                ralink,function = "gpio";
            };
            ephy_p4 {
                ralink,group = "ephy_p4";
                ralink,function = "gpio";
            };
            wdt {
                ralink,group = "wdt";
                ralink,function = "gpio";
            };
            pwm0 {
                ralink,group = "pwm0";
                ralink,function = "gpio";
            };
            pwm1 {
                ralink,group = "pwm1";
                ralink,function = "gpio";
            };
            sdxc {
                ralink,group = "sdmode";
                ralink,function = "gpio";
            };
            i2c {
                ralink,group = "i2c";
                ralink,function = "gpio";
            };
            spi_cs1 {
                ralink,group = "spi cs1";
                ralink,function = "gpio";
            };
        };
    };

    palmbus@10000000 {
        spi@b00 {
            status = "okay";

            pinctrl-names = "default";
            pinctrl-0 = <&spi_pins>, <&spi_cs1_pins>;

            m25p80@0 {
                #address-cells = <1>;
                #size-cells = <1>;
                compatible = "w25q128";
                reg = <0 0>;
                linux,modalias = "m25p80", "w25q128";
                spi-max-frequency = <40000000>;
                m25p,chunked-io = <31>;

                partition@0 {
                    label = "u-boot";
                    reg = <0x0 0x30000>;
                    read-only;
                };

                partition@30000 {
                    label = "u-boot-env";
                    reg = <0x30000 0x10000>;
                };

                factory: partition@40000 {
                    label = "factory";
                    reg = <0x40000 0x10000>;
                };

                partition@50000 {
                    label = "firmware";
                    reg = <0x50000 0x0fb0000>;
                };
            };

            spidev@1 {
                #address-cells = <1>;
                #size-cells = <1>;
                compatible = "spidev";
                reg = <1 0>;
                spi-max-frequency = <40000000>;
            };
        };
    };

    ethernet@10100000 {
        mtd-mac-address = <&factory 0x28>;
    };
    pio-keys-polled {
        compatible = "gpio-keys-polled";
        #address-cells = <1>;
        #size-cells = <0>;
        poll-interval = <20>;
        wps {
            label = "reset";
            gpios = <&gpio1 6 1>;
            linux,code = <0x211>;
        };
    };
    sx127x {
        compatible = "spi-gpio";
        status = "okay";
        #address-cells = <0x1>;
        #size-cells = <0>;
        gpio-sck = <&gpio1 8 1>;
        gpio-miso = <&gpio1 10 1>;
        gpio-mosi = <&gpio1 9 1>;
        cs-gpios = <&gpio0 11 0>, <&gpio0 1 0>, <&gpio0 6 0>, <&gpio1 7 0>;
        //cs-gpios = <&gpio0 1 0>, <&gpio0 6 0>, <&gpio1 7 0>;
        num-chipselects = <4>;
        sx12781 {
            compatible = "semtech,sx1278-1";
            reg = <0>;
            spi-max-frequency = <10000000>;
        };
        sx12782 {
            compatible = "semtech,sx1278-2";
            reg = <1>;
            spi-max-frequency = <10000000>;
        };
        sx12783 {
            compatible = "semtech,sx1278-3";
            reg = <2>;
            spi-max-frequency = <10000000>;
        };
        sx12784 {
            compatible = "semtech,sx1278-4";
            reg = <3>;
            spi-max-frequency = <10000000>;
        };

    };
    //si44xx {
    //  compatible = "spi-gpio";
    //  status = "okay";
    //  #address-cells = <0x1>;
    //    #size-cells = <0>;
    //  gpio-sck = <&gpio0 25 1>;
    //  gpio-miso = <&gpio0 27 1>;
    //  gpio-mosi = <&gpio0 26 1>;
    //  num-chipselects = <1>;
    //  cs-gpios = <&gpio0 24 0>;
    //  si4438 {
    //      compatible = "silabs,si4438";
    //      reg = <0>;
    //      spi-max-frequency = <500000>;
    //  };
    //};
};
